{
    "metadata": {
        "name": "deplo4"
    },
    "kind": "Deployment",
    "spec": {
        "template": {
            "metadata": {
                "labels": {
                    "run": "deplo4",
                    "test": "bbtbt"
                },
                "name": "deplo4"
            },
            "spec": {
                "containers": [
                    {
                        "name": "deplo4",
                        "resources": {
                            "requests": {
                                "memory": "128Mi",
                                "cpu": "100m"
                            }
                        },
                        "image": "ubuntu"
                    }
                ]
            }
        },
        "replicas": 1
    }
}

